{
  "name": "@scaleway/ui",
  "version": "0.123.0",
  "description": "Scaleway UI",
  "homepage": "https://github.com/scaleway/scaleway-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-ui"
  },
  "keywords": [
    "react",
    "reactjs",
    "ui"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1.22.x"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "packageManager": "yarn@3.1.0",
  "sideEffects": false,
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "jsnext:main": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:profile": "cross-env PROFILE=true yarn run build",
    "build:storybook": "STORYBOOK_ENVIRONMENT=production build-storybook",
    "check:deps": "npx depcheck . --skip-missing=true --ignores='bin,eslint,webpack,jest,husky,@semantic-release/*,@commitlint/*,@babel/*,babel-*'",
    "commit": "npx git-cz -a",
    "start": "STORYBOOK_ENVIRONMENT=development start-storybook -p 6006",
    "start:production": "STORYBOOK_ENVIRONMENT=production start-storybook -p 6006",
    "format": "prettier --write '**/*.{ts,tsx,js,json,md,mdx}'",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache --ext js,ts,tsx,mdx .",
    "prebuild": "shx rm -rf dist",
    "test:unit:coverage": "yarn run test:unit --coverage",
    "test:watch": "yarn run test --watch",
    "test:update": "yarn run test --updateSnapshot",
    "test": "LC_ALL=en_US.UTF-8 jest",
    "test:unit": "yarn test --testPathIgnorePatterns='src/__tests__/a11y.test.tsx'",
    "test:a11y": "yarn test src/__tests__/a11y.test.tsx",
    "prepare": "husky install",
    "size": "yarn build && size-limit"
  },
  "lint-staged": {
    "*.(j|t)s?(x)": [
      "prettier --write",
      "eslint --fix --ext js,jsx,ts,tsx"
    ],
    "*.mdx": [
      "prettier --write",
      "eslint --fix --ext mdx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "size-limit": [
    {
      "path": "dist/**/*.js",
      "limit": "220 kB",
      "webpack": false,
      "brotli": true,
      "running": false
    }
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/__stories__/**/*.{js,ts,tsx,mdx}",
      "!src/__tests__/a11y.test.tsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".reports",
          "outputName": "tests.xml"
        }
      ]
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@scaleway)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "dependenciesMeta": {
    "@react-spring/core": {
      "built": false
    }
  },
  "peerDependencies": {
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/cache": "11.6.0",
    "@emotion/eslint-plugin": "11.5.0",
    "@emotion/jest": "11.6.0",
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-url": "6.1.0",
    "@scaleway/eslint-config-react": "2.3.2",
    "@scaleway/jest-helpers": "1.1.4",
    "@scaleway/use-i18n": "3.1.22",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@size-limit/file": "7.0.3",
    "@size-limit/preset-big-lib": "7.0.3",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/testing-react": "0.0.22",
    "@storybook/theming": "6.3.12",
    "@svgr/rollup": "5.5.0",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest-axe": "3.5.3",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/zxcvbn": "4.4.1",
    "babel-loader": "8.2.3",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "2.25.0",
    "eslint": "8.2.0",
    "eslint-mdx": "1.16.0",
    "eslint-plugin-mdx": "1.16.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-axe": "5.0.1",
    "jest-junit": "13.0.0",
    "lint-staged": "12.1.2",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.2",
    "read-pkg": "7.0.0",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.60.0",
    "rollup-plugin-dts": "4.0.1",
    "rollup-plugin-postcss": "4.0.1",
    "rollup-plugin-visualizer": "5.5.2",
    "semantic-release": "18.0.0",
    "shx": "0.3.3",
    "size-limit": "7.0.3",
    "timekeeper": "2.2.0",
    "typescript": "4.5.2",
    "webpack": "5.61.0",
    "zxcvbn": "4.4.2"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@emotion/serialize": "1.0.2",
    "@nivo/bar": "0.74.0",
    "@nivo/core": "0.74.0",
    "@nivo/line": "0.74.0",
    "@nivo/pie": "0.74.0",
    "@scaleway/random-name": "3.0.0",
    "@storybook/react": "6.3.12",
    "@types/intl-tel-input": "17.0.4",
    "@types/react-datepicker": "4.3.1",
    "@xstyled/emotion": "2.5.0",
    "intl-tel-input": "17.0.13",
