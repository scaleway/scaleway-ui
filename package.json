    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "2.28.0",
    "eslint": "8.14.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-axe": "6.0.0",
    "jest-junit": "13.2.0",
    "jest-mock-random": "1.1.1",
    "lint-staged": "12.4.1",
    "loki": "0.29.0",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
