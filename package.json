    "@semantic-release/release-notes-generator": "10.0.3",
    "@size-limit/file": "7.0.8",
    "@size-limit/preset-big-lib": "7.0.8",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-actions": "6.4.22",
