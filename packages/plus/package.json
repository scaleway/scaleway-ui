{
  "name": "@ultraviolet/plus",
  "version": "0.15.1",
  "description": "Ultraviolet Plus",
  "homepage": "https://github.com/scaleway/ultraviolet#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/ultraviolet",
    "directory": "packages/plus"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "type:generate": "tsc --declaration -p tsconfig.build.json",
    "build": "vite build --config vite.config.ts && pnpm run type:generate",
    "build:profile": "npx vite-bundle-visualizer -c vite.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "reactjs",
    "ui"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.x",
    "pnpm": ">=9.x"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "sideEffects": false,
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@ultraviolet/icons": "workspace:*",
    "@ultraviolet/illustrations": "workspace:*",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@uiw/codemirror-extensions-langs": "4.21.25",
    "@uiw/codemirror-theme-material": "4.21.25",
    "@uiw/react-codemirror": "4.21.25",
    "@ultraviolet/icons": "workspace:*",
    "@ultraviolet/themes": "workspace:*",
    "@ultraviolet/ui": "workspace:*",
    "react-intersection-observer": "9.8.2"
  }
}
